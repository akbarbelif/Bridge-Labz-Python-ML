{
	"name": "Blob2Cosmos",
	"properties": {
		"description": "Copy Blob cvs to Cosmos ",
		"activities": [
			{
				"name": "CopyBlob2Cosmos",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "shockprice/input/Google_Stock_Price_Train.csv"
					},
					{
						"name": "Destination",
						"value": "IoT1"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSetting",
							"recursive": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSetting",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "DocumentDbCollectionSink",
						"nestingSeparator": ".",
						"writeBatchSize": 1000,
						"writeBehavior": "insert"
					},
					"enableStaging": false,
					"parallelCopies": 4,
					"enableSkipIncompatibleRow": true,
					"dataIntegrationUnits": 16
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_fxv",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_fxv",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}
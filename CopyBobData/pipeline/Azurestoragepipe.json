{
	"name": "Azurestoragepipe",
	"properties": {
		"description": "Azure blob storage pipeline containing Google shock google  Price",
		"activities": [
			{
				"name": "CopynProcess",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "SourceBlob",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/akbar.belif1@cgm4l.onmicrosoft.com/demoworkspace/demonotebook",
					"baseParameters": {
						"accountkey": {
							"value": "@pipeline().parameters.accountkey",
							"type": "Expression"
						},
						"accountname": {
							"value": "@pipeline().parameters.accountname",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricksls",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "SourceBlob",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSetting",
							"recursive": true,
							"wildcardFileName": "*.*"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSetting"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSetting"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSetting",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Trainshockprice",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Trainshockprice",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"accountname": {
				"type": "string",
				"defaultValue": "cgtrainingbatch22019"
			},
			"accountkey": {
				"type": "string",
				"defaultValue": "7+djZSOsUAc25tFUcIETU3YB3kKUuyqW+RJ5ickrm9cRPZl7XQ+a6Mozt7h3WQuL9p7NPofS6Lbh3N4rpOvbUQ=="
			},
			"input": {
				"type": "string",
				"defaultValue": "Google_Stock_Price_Test.csv"
			}
		},
		"annotations": []
	}
}
{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adfdemo786"
		},
		"AzureBlobStorage_lnk_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_lnk'"
		},
		"AzureDatabricksls_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksls'"
		},
		"CosmosDbls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbls'"
		},
		"week6reviewADlink_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'week6reviewADlink'"
		},
		"week6reviewlink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'week6reviewlink'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_lnk_connectionString": {
						"value": "[parameters('AzureBlobStorage_lnk_connectionString')]"
					},
					"AzureDatabricksls_accessToken": {
						"value": "[parameters('AzureDatabricksls_accessToken')]"
					},
					"CosmosDbls_connectionString": {
						"value": "[parameters('CosmosDbls_connectionString')]"
					},
					"week6reviewADlink_accessToken": {
						"value": "[parameters('week6reviewADlink_accessToken')]"
					},
					"week6reviewlink_connectionString": {
						"value": "[parameters('week6reviewlink_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}